# tech.lib 示例（8位浮点加法器）

# 浮点加法器单元定义
cell: FLOAT_ADDER8
  area: 10       # 单元面积（面积仅为示例）
  leakage_power: 0.2   # 静态功耗
  dynamic_power: 1.0  # 动态功耗

# 浮点格式定义
float_format: IEEE754   # 使用IEEE 754浮点数表示

# 时序信息
cell: FLOAT_ADDER8
  max_frequency: 500   # 最大工作频率（仅为示例）

# 精度和误差信息
cell: FLOAT_ADDER8
  precision: 8   # 浮点数精度
  rounding_error: 0.001   # 舍入误差

# 电源电压和温度相关信息
voltage: 1.2   # 电源电压
temperature: 25   # 温度

# 其他技术库信息
process_corner: typical   # 工艺角信息
